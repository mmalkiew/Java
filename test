private static KeyHolder keyHolder = new GeneratedKeyHolder();

	public void insertNewObject(Object object)
	{
		jdbcTemplate.update(new PreparedStatementCreator() 
		{           
            @Override
            public PreparedStatement createPreparedStatement(Connection connection) throws SQLException 
            {
                PreparedStatement ps = connection.prepareStatement(SQL_STATEMENT, Statement.RETURN_GENERATED_KEYS);
                ps.setString(1, name_object);
                
                return ps;
            }
        }, keyHolder);

		Integer idObject = keyHolder.getKey().intValue();
	}
